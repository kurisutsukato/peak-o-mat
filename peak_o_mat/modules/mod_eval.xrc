<?xml version="1.0" encoding="utf-8"?>
<resource>
  <object class="wxPanel" name="mod_eval">
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxStaticBoxSizer">
              <label>spline</label>
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxPanel" name="xrc_pan_spline">
                  <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                      <object class="wxToggleButton" name="xrc_btn_place_handles">
                        <label>pick control points</label>
                        <style/>
                      </object>
                      <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                      <border>2</border>
                    </object>
                    <object class="spacer">
                      <size>20,1</size>
                    </object>
                    <object class="sizeritem">
                      <object class="wxButton" name="xrc_btn_anchor">
                        <label>create set</label>
                        <style>wxBU_EXACTFIT</style>
                      </object>
                      <flag>wxALL|wxEXPAND|wxALIGN_CENTRE_VERTICAL</flag>
                      <border>2</border>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>with</label>
                      </object>
                      <flag>wxLEFT|wxRIGHT|wxALIGN_CENTRE_VERTICAL</flag>
                      <border>5</border>
                    </object>
                    <object class="sizeritem">
                      <object class="wxTextCtrl" name="xrc_txt_spline_pts">
                        <value>500</value>
                        <style>wxTE_RIGHT</style>
                      </object>
                      <option>1</option>
                      <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                      <border>2</border>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>points</label>
                      </object>
                      <flag>wxLEFT|wxALIGN_CENTRE_VERTICAL</flag>
                      <border>2</border>
                    </object>
                  </object>
                </object>
                <flag>wxALL|wxEXPAND</flag>
                <border>5</border>
              </object>
            </object>
            <flag>wxALL|wxEXPAND</flag>
            <border>5</border>
          </object>
          <object class="sizeritem">
            <object class="wxStaticBoxSizer">
              <label>evaluate equation</label>
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxBoxSizer">
                      <orient>wxHORIZONTAL</orient>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>y = </label>
                        </object>
                        <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                        <border>5</border>
                      </object>
                      <object class="sizeritem">
                        <object class="wxTextCtrl" name="xrc_txt_eq">
                          <value>sin(x)</value>
                          <size>400,-1</size>
                        </object>
                        <option>1</option>
                        <flag>wxALL|wxEXPAND|wxALIGN_CENTRE_VERTICAL</flag>
                        <border>2</border>
                      </object>
                    </object>
                    <flag>wxALL|wxEXPAND</flag>
                    <border>5</border>
                  </object>
                  <object class="sizeritem">
                    <object class="wxBoxSizer">
                      <orient>wxHORIZONTAL</orient>
                      <object class="sizeritem">
                        <object class="wxCheckBox" name="xrc_cb_range">
                          <label>range</label>
                          <checked>0</checked>
                        </object>
                        <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                        <border>2</border>
                      </object>
                      <object class="sizeritem">
                        <object class="wxPanel" name="xrc_pan_range">
                          <object class="wxBoxSizer">
                            <orient>wxHORIZONTAL</orient>
                            <object class="spacer">
                              <size>20,0</size>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText" name="">
                                <label>from</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxTextCtrl" name="xrc_txt_range_from">
                                <style>wxTE_RIGHT</style>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>to</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxTextCtrl" name="xrc_txt_range_to">
                                <style>wxTE_RIGHT</style>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>with</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxTextCtrl" name="xrc_txt_range_pts">
                                <value>500</value>
                                <style>wxTE_RIGHT</style>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>points</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                          </object>
                        </object>
                        <flag>wxALL|wxEXPAND</flag>
                      </object>
                    </object>
                    <flag>wxEXPAND</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxBoxSizer">
                      <orient>wxHORIZONTAL</orient>
                      <object class="sizeritem">
                        <object class="wxCheckBox" name="xrc_cb_fromset">
                          <label>from set</label>
                          <checked>0</checked>
                        </object>
                        <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                        <border>2</border>
                      </object>
                      <object class="sizeritem">
                        <object class="wxPanel" name="xrc_pan_fromset">
                          <object class="wxBoxSizer">
                            <orient>wxHORIZONTAL</orient>
                            <object class="spacer">
                              <size>20,0</size>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText" name="">
                                <label>x-axis from set</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxChoice" name="xrc_cmb_fromset">
                                <content>
                                  <item>s0</item>
                                  <item>s1</item>
                                </content>
                              </object>
                              <flag>wxALL|wxEXPAND|wxALIGN_CENTRE_VERTICAL</flag>
                              <border>2</border>
                            </object>
                          </object>
                        </object>
                        <option>1</option>
                        <flag>wxALL|wxEXPAND</flag>
                      </object>
                      <object class="spacer">
                        <size>0,0</size>
                        <option>1</option>
                      </object>
                      <object class="sizeritem">
                        <object class="wxButton" name="xrc_btn_load">
                          <label>Load</label>
                        </object>
                      </object>
                    </object>
                    <flag>wxEXPAND</flag>
                  </object>
                </object>
                <option>1</option>
                <flag>wxEXPAND</flag>
              </object>
            </object>
            <flag>wxALL|wxEXPAND</flag>
            <border>5</border>
          </object>
        </object>
        <option>1</option>
        <flag>wxALL|wxEXPAND</flag>
        <border>5</border>
      </object>
    </object>
    <exstyle>wxWS_EX_VALIDATE_RECURSIVELY</exstyle>
  </object>
</resource>